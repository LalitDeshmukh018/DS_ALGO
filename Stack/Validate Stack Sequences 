// while wrtitng the code got stuck out of bound
// so careful
// one thing whenever popping check whether stack is not empty
// manage the index of getting out of bound




class Solution {
    public boolean validateStackSequences(int[] pushed, int[] popped) {
        Stack<Integer> st = new Stack<>();
        int n = pushed.length;
        int j =0;
        for(int i =0; i<n; i++){
            while(!st.empty() && popped[j] == st.peek()){
                st.pop();
                j++;
            }
            st.push(pushed[i]);
        }
         while(!st.empty() && popped[j] == st.peek()){
                st.pop();
                j++;
        }
        return st.empty() && j==n; 
    }
}