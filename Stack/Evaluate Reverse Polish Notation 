class Solution {
    public int evalRPN(String[] tokens) {
       int n = tokens.length;
       Stack<Integer> st = new Stack();

       for (String ch : tokens){
        if(ch.equals("+")){
            int first = st.pop();
            int second = st.pop();
            st.push(first+second);
        }
        else if(ch.equals("-")){
            int second = st.pop();
            int first = st.pop();
            st.push(first-second);
        }
        else if(ch.equals("*")){
            int second = st.pop();
            int first = st.pop();
            st.push(first*second);
        }   
        else if (ch.equals("/")){
            int second = st.pop();
            int first = st.pop();
            st.push(first/second);
        }
        else {
            st.push(Integer.parseInt(ch));
        }
        
        
            }
            return st.peek();
    }
}